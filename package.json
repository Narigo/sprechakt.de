		"reimemonster": "0.2.1",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-markdown": "0.2.3",
		"svelte-preprocess": "5.0.4",
		"ts-jest": "29.1.0",
		"ts-node": "10.9.1",
		"tslib": "2.5.2",
		"typescript": "5.0.4",
		"vite": "4.3.9",
		"zx": "7.2.2"
	},
	"type": "module",
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		}
	},
	"packageManager": "pnpm@8.5.1"
